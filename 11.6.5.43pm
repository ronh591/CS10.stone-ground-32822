void func(int a[][7]) {
  for (int i = 0; i < 5; i++) {
    for (int j = 0; j < 7; j++) {
      cout << a[i][j] << " ";
    }
    cout << endl;
  }
}

int main() {
  int a[5][7];
  for (int i = 0; i < 5; i++) {
    for (int j = 0; j < 7; j++) {
      a[i][j] = i * j;
    }
  }

  func(a);

  return 0;
}
